#include <stdio.h>

void findMaxMin(int* nums, int n) {
    if (n <= 0) return;

    int max = nums[0];
    int min = nums[0];

    for (int i = 1; i < n; i++) {
        if (nums[i] > max) {
            max = nums[i];
        }
        if (nums[i] < min) {
            min = nums[i];
        }
    }

    printf("Max: %d\n", max);
    printf("Min: %d\n", min);
}

int main() {
    int n;
    if (scanf("%d", &n) != 1) return 0;

    int nums[n];
    for (int i = 0; i < n; i++) {
        scanf("%d", &nums[i]);
    }

    findMaxMin(nums, n);

    return 0;
}
